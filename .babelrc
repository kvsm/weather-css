{
  "presets": ["react", "es2015", "stage-0", "stage-1"],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }],
        ["transform-object-rest-spread"]
      ]
    }
  }
}
